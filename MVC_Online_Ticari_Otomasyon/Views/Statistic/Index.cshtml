                <div class="clearfix"></div>

            </div>

            <div class="dropdown-divider"></div>   @*--------------------------------------------*@


            <div class="visitors" style="background-color:#95a5a6">

                <div class="icon-text d-inline-block">

                    <label><strong class="fs-2">Total Number of Laptop(In Products) :</strong></label> <h5 class=" d-inline-block">@ViewBag.TotalNumberOfLaptop</h5>
                </div>

                <div class="clearfix"></div>

            </div>

            <div class="dropdown-divider"></div>   @*--------------------------------------------*@


            <div class="clearfix"></div>

        </div>

    </div>

    <div class="clearfix"></div>

    <div class="total-content">

        <div class="list_of_members">

            <div class="sales" style="background-color:#f39c12">

                <div class="icon-text d-inline-block">

                    <label><strong class="fs-2">Top Seller :</strong></label> <h5 class=" d-inline-block">@ViewBag.TopSeller</h5>
                </div>

                <div class="clearfix"></div>

            </div>

            <div class="dropdown-divider"></div>   @*--------------------------------------------*@


            <div class="new-users" style="background-color:#d35400">

                <div class="icon-text d-inline-block">

                    <label><strong class="fs-2">Amount in Safe/Money Case :</strong></label> <h5 class=" d-inline-block">@ViewBag.TotalAmountInSafe</h5>
                </div>

                <div class="clearfix"></div>

            </div>

            <div class="dropdown-divider"></div>   @*--------------------------------------------*@


            <div class="orders" style="background-color:#c0392b">

                <div class="icon-text d-inline-block">

                    <label><strong class="fs-2">Sales of Today :</strong></label> <h5 class=" d-inline-block">@ViewBag.SalesOfToday</h5>
                </div>

                <div class="clearfix"></div>

            </div>

            <div class="dropdown-divider"></div>   @*--------------------------------------------*@


            <div class="visitors" style="background-color:#7f8c8d">

                <div class="icon-text d-inline-block">

                    <label><strong class="fs-2">Today Safe :</strong></label> <h5 class=" d-inline-block">@ViewBag.TotalAmountSalesOfToday</h5>
