<body>
    <h2>Statistics</h2>
    <div class="total-content">

        <div class="list_of_members">

            <div class="sales" style="background-color:#1abc9c">

                <div class="icon-text d-inline-block">

                    <label><strong class="fs-2">Total Customer :</strong></label>   <h5 class="d-inline-block">@ViewBag.TotalNumberOfCustomer</h5>

                </div>

                <div class="clearfix"></div>

            </div>

            <div class="dropdown-divider"></div>    @*--------------------------------------------*@

            <div class="new-users" style="background-color:#2ecc71">

                <div class="icon-text  d-inline-block">
                    <label><strong class="fs-2">Total Number of Products :</strong></label>  <h5 class=" d-inline-block">@ViewBag.TotalNumberOfProducts</h5>
                </div>

                <div class="clearfix"></div>

            </div>

            <div class="dropdown-divider"></div>    @*--------------------------------------------*@


            <div class="orders" style="background-color:#3498db">

                <div class="icon-text d-inline-block">

                    <label><strong class="fs-2">Total Number of Employee :</strong></label> <h5 class=" d-inline-block">@ViewBag.TotalNumberOfEmployees</h5>
                </div>

                <div class="clearfix"></div>

            </div>

            <div class="dropdown-divider"></div>   @*--------------------------------------------*@


            <div class="visitors" style="background-color:#9b59b6">

                <div class="icon-text d-inline-block">

                    <label><strong class="fs-2">Number of Categories :</strong></label> <h5 class=" d-inline-block">@ViewBag.TotalNumberOfCategories</h5>
                </div>

                <div class="clearfix"></div>

            </div>

            <div class="dropdown-divider"></div>   @*--------------------------------------------*@

        </div>

    </div>

    <div class="clearfix"></div>

    <div class="total-content">

        <div class="list_of_members">

            <div class="sales" style="background-color:#16a085">

                <div class="icon-text d-inline-block">
                    <label><strong class="fs-2">Total Inventory :</strong></label> <h5 class=" d-inline-block">@ViewBag.TotalNumberOfInventory</h5>
                </div>

                <div class="clearfix"></div>

            </div>

            <div class="dropdown-divider"></div>   @*--------------------------------------------*@


            <div class="new-users" style="background-color:#27ae60">

                <div class="icon-text d-inline-block">

                    <label><strong class="fs-2">Total Number of Brand :</strong></label> <h5 class=" d-inline-block">@ViewBag.TotalNumberOfBrands</h5>
                </div>

                <div class="clearfix"></div>

            </div>

            <div class="dropdown-divider"></div>   @*--------------------------------------------*@


            <div class="orders" style="background-color:#2980b9">

                <div class="icon-text d-inline-block">

                    <label><strong class="fs-2">Critic Level :</strong></label> @*<h5 class=" d-inline-block">@Html.ListBox("CiriticLevelProductsList", (IEnumerable<SelectListItem>)ViewBag.CiriticLevelProductsList)</h5>*@


                    @*@Html.DropDownList("CiriticLevelProductsList", (List<SelectListItem>)ViewBag.CiriticLevelProductsList, new { @class = "form-control" })*@


                    @*<ol>
                        @foreach (var item in ViewBag.CriticLevelProductList)
                        {
                            <li>@item</li>
                        }
                    </ol>*@





                </div>

                <div class="clearfix"></div>

            </div>

            <div class="dropdown-divider"></div>   @*--------------------------------------------*@


            <div class="visitors" style="background-color:#8e44ad">

                <div class="icon-text d-inline-block">

                    <label><strong class="fs-2">Maximum Priced Product :</strong></label> <h5 class=" d-inline-block">@ViewBag.MaximumPricedProduct</h5>
                </div>

                <div class="clearfix"></div>

            </div>

            <div class="dropdown-divider"></div>   @*--------------------------------------------*@


            <div class="clearfix"></div>

        </div>

    </div>

    <div class="clearfix"></div>

    <div class="total-content">

        <div class="list_of_members">

            <div class="sales" style="background-color:#f1c40f">

                <div class="icon-text d-inline-block">

                    <label><strong class="fs-2">Minimum Priced Product :</strong></label> <h5 class=" d-inline-block">@ViewBag.MinimumPricedProduct</h5>
                </div>

                <div class="clearfix"></div>

            </div>

            <div class="dropdown-divider"></div>   @*--------------------------------------------*@


            <div class="new-users" style="background-color:#e67e22">

                <div class="icon-text d-inline-block">

                    <label><strong class="fs-2">Max Marka :</strong></label> <h5 class=" d-inline-block">@ViewBag.TotalNumberOfBrand</h5>
                </div>

                <div class="clearfix"></div>

            </div>

            <div class="dropdown-divider"></div>   @*--------------------------------------------*@


            <div class="orders" style="background-color:#e74c3c">

